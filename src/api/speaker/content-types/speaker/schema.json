{
  "kind": "collectionType",
  "collectionName": "speakers",
  "info": {
    "singularName": "speaker",
    "pluralName": "speakers",
    "displayName": "Speaker",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "biography": {
      "type": "blocks"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "highlight": {
      "type": "boolean",
      "default": false
    },
    "talks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::talk.talk",
      "mappedBy": "speakers"
    },
    "socials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::link.link"
    },
    "email": {
      "type": "string",
      "required": false
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "speaker"
    }
  }
}
